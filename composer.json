{
    "name": "innocode-digital/wp-cli-scaffold-theme-command",
    "version": "1.2.0",
    "description": "Generates starter code for a theme based on Innocode theme skeleton.",
    "type": "wp-cli-package",
    "homepage": "https://github.com/innocode-digital/wp-cli-scaffold-theme-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Viktor Kuliebiakin",
            "email": "viktor.kuliebiakin@innocode.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Innocode\\ScaffoldTheme\\": "src/"
        },
        "files": [
            "scaffold-theme-command.php"
        ]
    },
    "require-dev": {
        "wp-cli/wp-cli": "*",
        "behat/behat": "~2.5"
    },
    "require": {
        "ext-json": "*",
        "wp-cli/scaffold-command": "*",
        "knplabs/github-api": "^2.10",
        "php-http/guzzle6-adapter": "^1.1",
        "gettext/gettext": "^4.6"
    }
}
